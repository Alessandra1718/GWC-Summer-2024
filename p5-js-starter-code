function setup() {
  // This code only runs once, sets canvas size
  createCanvas(200, 200);
  background('lightyellow');
  
}

function draw() {
  // This code will run repeatedly
  // Fill color
  fill('green');
 // Draw circle  
  ellipse(30, 110, 50 , 50);
  // x, y( up to down ), lenght of elipse, height of elipse itself 
}
